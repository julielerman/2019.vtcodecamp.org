<!doctype html>
<html>

<head>
    <meta name="viewport" content="initial-scale=1.0, width=device-width" > 
    <link rel="icon" href="favicon.ico" type="image/x-icon" />

    <!-- Primary Meta Tags -->
    <title>Vermont Code Camp | {{title}}</title>
    <meta name="title" content="Vermont Code Camp | {{title}}">
    <meta name="description" content="Vermont Code Camp is a full day event that brings together technology community members, students and professionals from around Vermont and beyond.">

    <!-- Open Graph / Facebook -->
    <meta property="og:type" content="website">
    <meta property="og:url" content="https://www.vtcodecamp.org/">
    <meta property="og:title" content="Vermont Code Camp | {{title}}">
    <meta property="og:description" content="Vermont Code Camp is a full day event that brings together technology community members, students and professionals from around Vermont and beyond.">
    <meta property="og:image" content="/assets/event-sign.jpg">

    <!-- Twitter -->
    <meta property="twitter:card" content="summary_large_image">
    <meta property="twitter:url" content="https://www.vtcodecamp.org/">
    <meta property="twitter:title" content="Vermont Code Camp | {{title}}">
    <meta property="twitter:description" content="Vermont Code Camp is a full day event that brings together technology community members, students and professionals from around Vermont and beyond.">
    <meta property="twitter:image" content="/assets/event-sign.jpg">


    <link rel="stylesheet" type="text/css" href="/assets/styles.css">
</head>

